<?xml version="1.0" encoding="ISO-8859-1" ?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
           http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5">

    <!-- A continuacion deben declararse todas las Interfaces que se implementan como
         Servlets en el sistema. Para cada Interfaz deben declararse los tags
         <servlet> y <servlet-mapping>
     -->

    <servlet>
      <servlet-name>InicioS</servlet-name>
      <servlet-class>interfaces.InterfazInicioS</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InicioS</servlet-name>
        <url-pattern>/InicioS</url-pattern>
    </servlet-mapping>

    <servlet>
      <servlet-name>ConsultarProyecto</servlet-name>
      <servlet-class>interfaces.InterfazConsultarProyecto</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ConsultarProyecto</servlet-name>
        <url-pattern>/ConsultarProyecto</url-pattern>
    </servlet-mapping>

    <servlet>
      <servlet-name>Mapeo</servlet-name>
      <servlet-class>interfaces.InterfazMapeo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Mapeo</servlet-name>
        <url-pattern>/Mapeo</url-pattern>
    </servlet-mapping>

    <servlet>
      <servlet-name>RegistrarProyecto</servlet-name>
      <servlet-class>interfaces.InterfazRegistrarProyecto</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegistrarProyecto</servlet-name>
        <url-pattern>/RegistrarProyecto</url-pattern>
    </servlet-mapping>

    <servlet>
      <servlet-name>UsuarioN</servlet-name>
      <servlet-class>interfaces.InterfazUsuarioN</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UsuarioN</servlet-name>
        <url-pattern>/UsuarioN</url-pattern>
    </servlet-mapping>

    <servlet>
      <servlet-name>CerrarS</servlet-name>
      <servlet-class>interfaces.InterfazCerrarS</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CerrarS</servlet-name>
        <url-pattern>/CerrarS</url-pattern>
    </servlet-mapping>

    <servlet>
      <servlet-name>CambioP</servlet-name>
      <servlet-class>interfaces.InterfazCambioPrivi</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CambioP</servlet-name>
        <url-pattern>/CambioP</url-pattern>
    </servlet-mapping>
